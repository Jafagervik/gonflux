fun sum(a, acc) ->
    ret match a =
        [] => acc,
        h :: t => h + sum(a, acc+t),
        h :: [] => acc + h
end

proc main() ->  
    a = 1..5   // List from 1 to 5, can be changed
    b = 1..10..2 // List from 1 to 10 with step of 2, cannnot change

    print(sum(a, 0)) // call function
    print(sum(b, 2)) // call function 
end

main()
